@model RegionEntity
<div class="row cl pd-10">
	<label class="form-label col-xs-4 col-sm-3">区域ID：</label>
	<div class="formControls col-xs-8 col-sm-9">
		@Model.RegionID
	</div>
</div>
<div class="row cl pd-10">
	<label class="form-label col-xs-4 col-sm-3">国家名称：</label>
	<div class="formControls col-xs-8 col-sm-9">
		@Model.Country
	</div>
</div>
<div class="row cl pd-10">
	<label class="form-label col-xs-4 col-sm-3">省份名称：</label>
	<div class="formControls col-xs-8 col-sm-9">
		@Model.Province
	</div>
</div>
<div class="row cl pd-10">
	<label class="form-label col-xs-4 col-sm-3">城市名称：</label>
	<div class="formControls col-xs-8 col-sm-9">
		@Model.City
	</div>
</div>
<div class="row cl pd-10">
	<label class="form-label col-xs-4 col-sm-3">区域名称：</label>
	<div class="formControls col-xs-8 col-sm-9">
		@Model.Area
	</div>
</div>
<div class="row cl pd-10">
	<label class="form-label col-xs-4 col-sm-3">邮政编码：</label>
	<div class="formControls col-xs-8 col-sm-9">
		@Model.PostCode
	</div>
</div>
<div class="row cl pd-10">
	<label class="form-label col-xs-4 col-sm-3">区号：</label>
	<div class="formControls col-xs-8 col-sm-9">
		@Model.AreaCode
	</div>
</div>
<div class="row cl pd-10">
	<div class="col-xs-8 col-sm-9 col-xs-offset-4 col-sm-offset-3">
		<button class="btn btn-default radius" type="button" onclick="layer_close()">取消</button>
	</div>
</div>